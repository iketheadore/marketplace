{
  "name": "kittycash-marketplace",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "prepare-lib": "gulp inline-build-templates copy-environments copy-translations",
    "build-lib": "node_modules/.bin/ngc -p ./tsconfig.lib.json",
    "package-lib": "gulp package-json && cd lib && npm pack"
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@angular/service-worker": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.9",
    "@ngx-translate/core": "^9.0.1",
    "@types/core-js": "^0.9.46",
    "bootstrap": "4.0.0",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
    "gulp": "^4.0.0",
    "gulp-inline-ng2-template": "^4.1.0",
    "gulp-rename": "^1.2.2",
    "intl-tel-input": "^12.1.10",
    "lodash": "^4.17.4",
    "ng-recaptcha": "^3.0.3",
    "ng2-bootstrap-modal": "^1.0.1",
    "ng4-intl-phone": "^1.2.1",
    "ngx-bootstrap": "^2.0.2",
    "ngx-pagination": "^3.1.0",
    "ngx-qrcode2": "^0.1.0",
    "rxjs": "^5.5.5",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "~1.6.5",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@ngx-rocket/scripts": "^2.0.0",
    "@types/jasmine": "^2.5.52",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.52",
    "@types/node": "^8.0.10",
    "codelyzer": "^4.1.0",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "~2.9.1",
    "jasmine-spec-reporter": "^4.1.0",
    "protractor": "^5.3.2",
    "puppeteer": "^1.0.0",
    "stylelint": "~8.4.0",
    "stylelint-config-recommended-scss": "~3.0.0",
    "stylelint-config-standard": "~18.0.0",
    "stylelint-scss": "~2.2.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
